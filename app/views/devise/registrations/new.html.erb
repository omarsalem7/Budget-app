<header class="d-flex justify-content-between align-items-center header">
  <img style="width:30px;height: 30px;"src="https://img.icons8.com/officel/344/back.png"/>
  <h5>Register</h5>
</header>
<div class="mx-2">
  <%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
    <%= render "devise/shared/error_messages", resource: resource %>
    <div>
      <%= f.text_field :name , autofocus: true, placeholder: "Name" %>
    </div>
    <div class="field">
      <%= f.email_field :email, autocomplete: "email",placeholder: "Email" %>
    </div>
    <div class="field">
      <%= f.password_field :password, autocomplete: "new-password",placeholder: "Password" %>
    </div>
    <div class="field">
      <%= f.password_field :password_confirmation, autocomplete: "new-password", placeholder: "New-password" %>
    </div>
    <div class="actions">
      <%= f.submit "Sign up" %>
    </div>
  <% end %>
  <div class="center-links">
    <%= render "devise/shared/links" %>
  </div>
</div>
